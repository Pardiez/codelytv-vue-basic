{
  "name": "quasar-app",
  "productName": "Quasar Mosica",
  "version": "0.0.1",
  "description": "Mosica Sample with Quasar",
  "author": "Coding Stones<yeah@codingstones.com>",
  "scripts": {
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js",
    "lint": "eslint --ext .js,.vue src",
    "unit": "npm run jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.js",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!quasar-framework)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test/e2e"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "fetch-jsonp": "^1.1.3",
    "js-beautify": "^1.6.14",
